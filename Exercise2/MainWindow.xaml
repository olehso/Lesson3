<Window x:Class="Exercise2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exercise2"
        mc:Ignorable="d"
        Title="Lesson №3 Ex2" Height="436.5" Width="761">
    <Grid x:Name="Arr" Background="Gray">
        <Button x:Name="button" Content="Сгенерировать массив" HorizontalAlignment="Left" Margin="91,218,0,0" VerticalAlignment="Top" Width="204" Height="36" FontSize="16" Click="button_Click"/>
        <Button x:Name="button1" Content="Подсчет элементов" HorizontalAlignment="Left" Margin="482,218,0,0" VerticalAlignment="Top" Width="165" Height="36" FontSize="16" Click="button1_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="37" Margin="71,131,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240" TextChanged="textBox_TextChanged" FontSize="16"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="37" Margin="446,131,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240" TextChanged="textBox1_TextChanged" FontSize="16"/>
        <Button x:Name="button2" Content="Выход" HorizontalAlignment="Left" Margin="614,327,0,0" VerticalAlignment="Top" Width="104" Height="48" FontSize="18" Click="button2_Click" RenderTransformOrigin="-0.25,-0.375"/>
        <Label x:Name="label" Content="Исходный массив" HorizontalAlignment="Left" Margin="108,92,0,0" VerticalAlignment="Top" FontSize="18" Foreground="White"/>
        <Label x:Name="label1" Content="Кол-во неповторяющихся эл" HorizontalAlignment="Left" Margin="439,92,0,0" VerticalAlignment="Top" FontSize="18" Foreground="White"/>
    </Grid>
</Window>
